{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "Domain": "<<Your Domain>>", //something like this <<Yourdomain>>.onmicrosoft.com
    "TenantId": "<<Your Tenant Id>>",
    "ClientId": "<<Your API App Registration App Id>>",
    "DefaultApiScope": "api://<<Your API App Id>>/.default"
  }
}